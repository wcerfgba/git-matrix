{
  "name": "git-matrix",
  "version": "0.0.3",
  "description": "Generate a table showing activity on files by users.",
  "keywords": [
    "git",
    "analytics",
    "agile",
    "team",
    "utility"
  ],
  "homepage": "https://github.com/wcerfgba/git-matrix",
  "license": "Unlicensed",
  "author": "John Preston <wcerfgba@riseup.net>",
  "main": "lib/index.js",
  "bin": {
    "git-matrix": "lib/index.js"
  },
  "scripts": {
    "clean": "rm -rf lib/*",
    "compile": "coffee -cmb --no-header -o lib/ src/",
    "shebangify": "sed -i '1s;^;#!/usr/bin/env node\\n;' lib/index.js",
    "check": "flow lib/",
    "chmod": "chmod +x lib/index.js",
    "build": "npm run clean && npm run compile && npm run shebangify && npm run check ; npm run chmod",
    "watch": "chokidar src/ -c 'npm run build'"
  },
  "dependencies": {
    "lodash": "^4.17.10",
    "yargs": "^12.0.1"
  },
  "devDependencies": {}
}
